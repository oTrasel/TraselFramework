{
    "name": "trase/trasel-framework",
    "autoload": {
        "psr-4": {
            "Routes\\": "app/Routes/",
            "Controllers\\": "app/Controllers/",
            "Models\\": "app/Models/",
            "Helpers\\": "app/Helpers/",
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "Lucas",
            "email": "trasel.lucas02@gmail.com"
        }
    ],
    "require": {
        "vlucas/phpdotenv": "^5.6"
    },
    "scripts": {
        "make:migration": "php app/Bin/make-migration.php",
        "migrate": "php app/bin/migrate.php up",
        "migrate:rollback": "php app/Bin/migrate.php rollback",
        "make:model": "php app/Bin/make-model.php"
    }
}
