<IfModule mod_rewrite.c>
    RewriteEngine On


    RewriteCond %{REQUEST_URI} !^/index\.php$


    RewriteRule ^(composer\.json|composer\.lock|\.env|\.git|\.htaccess|Architecture\.drawio|Processos\.drawio|readme\.md|LICENSE|vendor|app|\.vscode) - [F,L]


    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^ index.php [QSA,L]
</IfModule>
